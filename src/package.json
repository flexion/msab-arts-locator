{
  "name": "msab-art-locator-client",
  "private": true,
  "version": "0.0.1",
  "description": "Front end client for MSAB mobile Arts Locator application.",
  "author": "Flexion Inc.",
  "license": "MIT",
  "main": "sass/main.scss",
  "scripts": {
    "debug": "node --inspect-brk node_modules/.bin/jest --runInBand .*test\\.js$",
    "fix": "prettier --write",
    "test": "jest",
    "build-task:scss-compile": "node-sass sass/ -o dist/css",
    "build-task:autoprefixer": "postcss dist/css/*.css --use autoprefixer -d dist/css",
    "sass:build": "yarn build-task:scss-compile && yarn build-task:autoprefixer",
    "build-css": "node-sass-chokidar --include-path ./sass --include-path ./node_modules sass/ -o sass/",
    "watch-css": "yarn build-css && node-sass-chokidar --include-path ./sass --include-path ./node_modules sass/ -o sass/ --watch --recursive",
    "test:watch": "jest --watch .*test\\.js$",
    "test:unit": "jest --config jest-unit.config.js ./src/.*test\\.js$",
    "test:e2e": "jest --config jest-e2e.config.js ./e2e/.*test\\.js$",
    "test:pa11y": "pa11y-ci --config pa11y-ci.config.json",
    "test:all": "yarn lint && yarn test && yarn test:pa11y && yarn cypress",
    "lint": "eslint . && eslint ./**/*.jsx && stylelint ./**/*.scss",
    "lint:fix": "eslint --fix . && eslint --fix ./**/*.jsx && stylelint --fix ./**/*.scss",
    "start": "rm -rf dist && npm-run-all -p watch-css start-js",
    "start-js": "yarn codeVersion && MSAB_ENV=dev parcel --no-cache index.pug  --port 5678",
    "codeVersion": "echo \"module.exports = { version: '$(git describe)' };\" > environments/client/version.js",
    "build": "yarn codeVersion && MSAB_ENV=prod parcel build  --no-source-maps index.pug"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flexion/msab-arts-locator.git"
  },
  "bugs": {
    "url": "https://github.com/flexion/msab-arts-locator/issues"
  },
  "homepage": "https://github.com/flexion/msab-arts-locator#readme",
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@cerebral/react": "^4.1.0",
    "@miovision/eslint-plugin-disallow-date": "^2.0.0",
    "ajv": "^8.6.0",
    "ajv-errors": "^3.0.0",
    "aws-lambda-multipart-parser": "^0.1.3",
    "aws-xray-sdk": "^3.3.3",
    "axios": "^0.21.1",
    "babel-plugin-cerebral": "^1.0.1",
    "bloomer": "^0.6.5",
    "bulma": "^0.9.2",
    "cerebral": "^5.1.0",
    "conversions": "^1.5.2",
    "core-js": "3.14.0",
    "dynamodb-geo": "^0.4.0",
    "file-type": "^16.5.0",
    "format-url": "^1.1.0",
    "geolib": "^3.3.1",
    "jsonwebtoken": "^8.5.1",
    "node-sass-chokidar": "^1.5.0",
    "nodemailer": "^6.6.1",
    "papaparse": "^5.3.1",
    "pug": "^3.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-recaptcha": "^2.1.0",
    "riot-route": "^3.1.4",
    "sass": "^1.35.1",
    "sugarss": "^4.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "aws-sdk": "^2.931.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^27.0.2",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.0",
    "eslint-plugin-sort-requires": "^2.1.0",
    "fullstack-serverless": "^0.8.0",
    "jest": "^27.0.4",
    "jest-html-reporter": "^3.4.0",
    "node-sass": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.3.1",
    "serverless": "^2.46.0",
    "serverless-stack-output": "^0.2.3",
    "stylelint": "^13.13.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-standard": "^22.0.0",
    "xhr-mock": "^2.4.1"
  },
  "resolutions": {
    "sugarss": "^3.0.3",
    "qs": "^6.3.2"
  }
}